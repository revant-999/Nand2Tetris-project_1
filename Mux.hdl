/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel , out=z );
    And(a=a , b= z, out= j);
    And(a=b , b=sel , out= k);
    Or(a= j, b=k , out=out );
}